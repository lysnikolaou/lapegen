@subheader "from ast import literal_eval\n"

start: expr_stmt* ENDMARKER
expr_stmt: expr (NEWLINE | ^NoNewline)

expr:
  | expr '+' ~ term
  | expr '-' ~ term
  | term

term:
  | '-' ~ term
  | '+' ~ term
  | term '*' ~ factor
  | term '/' ~ factor
  | term '//' ~ factor
  | factor

factor:
  | atom ('**' | ^InvalidOperator) ~ factor
  | ^(InvalidOperator) atom

atom:
  | STRING
  | NUMBER
  | '(' ~ expr ')'
  | ^IvalidAtom
