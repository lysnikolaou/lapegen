start: expr_stmt* ENDMARKER
expr_stmt: expr NEWLINE

expr:
  | expr '+' ~ term
  | expr '-' ~ term
  | term

term:
  | '-' ~ term
  | '+' ~ term
  | term '*' ~ factor
  | term '/' ~ factor
  | term '//' ~ factor
  | factor

factor:
  | atom '**' ~ factor
  | atom

atom:
  | STRING
  | NUMBER
  | '(' ~ expr (')' | ^ParenNotClosing)
  | ^IvalidAtom
